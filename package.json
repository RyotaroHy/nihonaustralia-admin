{
  "name": "nihonaustralia-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "format": "prettier --write .",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "test": "jest --watch",
    "test:ci": "jest",
    "test:coverage": "jest --coverage",
    "seed": "tsx scripts/seed.ts",
    "seed:admin": "tsx scripts/seeds/seedAdminUsers.ts",
    "migrate:admin": "tsx scripts/add-admin-columns.ts",
    "setup:admin": "tsx scripts/setup-admin-db.ts",
    "setup:admin:local": "tsx scripts/setup-admin-db.ts --local",
    "deploy:admin": "tsx scripts/deploy-admin.ts",
    "db:link": "npx supabase link",
    "db:push": "npx supabase db push --linked",
    "db:pull": "npx supabase db pull --linked",
    "db:diff": "npx supabase db diff --linked"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write",
      "tsc --noEmit --skipLibCheck --pretty false"
    ],
    "**/*.{json,md,yml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@sentry/nextjs": "^9.24.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/react-query-devtools": "^5.76.2",
    "apexcharts": "^3.46.0",
    "dotenv": "^16.5.0",
    "flowbite-react": "^0.11.7",
    "next": "^15.1.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.5.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.7.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.2",
    "msw": "^2.8.7",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "supabase": "^2.20.12",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "web-streams-polyfill": "^4.1.0",
    "whatwg-fetch": "^3.6.20"
  }
}
